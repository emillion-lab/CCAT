{i:99,t:"–¢–µ–º–∞ 6",ty:"SIGNS",q:"–ó–∞–±—Ä–∞–Ω—è–≤–∞ >3.5—Ç –∏–∑–ø—Ä–µ–≤–∞—Ä–≤–∞–Ω–µ:",a:["–ú–ü–° –±–µ–∑ –º–æ—Ç–æ—Ä","–≤—Å–∏—á–∫–∏","–ª–µ–∫–∏"],c:0,m:"üí° –ë–µ–∑ –º–æ—Ç–æ—Ä–∏"},
{i:100,t:"–¢–µ–º–∞ 6",ty:"RULES",q:"–ü—Ä–∏ –∫–æ–ª–æ–Ω–∞:",a:["–Ω—è–º–∞–º","—Å–ø–∏—Ä–∞–º –ø—Ä–æ–ø—É—Å–∫–∞–º","—Å–∏–≥–Ω–∞–ª"],c:1,m:"üí° –°–ø–∏—Ä–∞–º"},
];

let S={ci:0,f:'all',ans:new Set(),cor:new Set(),wr:new Map()};

function init(){
  load();
  upd();
  render();
  setupFilters();
}

function load(){
  const d=localStorage.getItem('catC100');
  if(d){
    const p=JSON.parse(d);
    S.ans=new Set(p.ans||[]);
    S.cor=new Set(p.cor||[]);
    S.wr=new Map(p.wr||[]);
  }
}

function save(){
  localStorage.setItem('catC100',JSON.stringify({
    ans:Array.from(S.ans),
    cor:Array.from(S.cor),
    wr:Array.from(S.wr)
  }));
}

function upd(){
  const fq=filt();
  document.getElementById('totalQ').textContent=fq.length;
  document.getElementById('learnedQ').textContent=S.ans.size;
  
  const pr=fq.length>0?Math.round((S.ci/fq.length)*100):0;
  document.getElementById('progressQ').textContent=pr+'%';
  document.getElementById('progressBar').style.width=pr+'%';
  
  const acc=S.ans.size>0?Math.round((S.cor.size/S.ans.size)*100):0;
  document.getElementById('accuracyQ').textContent=acc+'%';
}

function filt(){
  if(S.f==='all')return Q;
  return Q.filter(q=>q.ty===S.f);
}

function render(){
  const fq=filt();
  if(!fq.length){
    document.getElementById('quizContainer').innerHTML='<p>–ù—è–º–∞ –≤—ä–ø—Ä–æ—Å–∏</p>';
    return;
  }
  
  const q=fq[S.ci];
  const isAns=S.ans.has(q.i);
  
  let h=`<div class="question-card">
    <div class="topic-header">${q.t}</div>
    <div class="question-header">
      <span class="question-type type-${q.ty}">${lbl(q.ty)}</span>
      <span>${S.ci+1}/${fq.length}</span>
    </div>
    <div class="question-text">${q.q}</div>
    <div class="answers">`;
  
  q.a.forEach((ans,idx)=>{
    const isCor=idx===q.c;
    let cls='answer-btn';
    if(isAns){
      if(isCor)cls+=' correct';
      else if(S.wr.has(q.i)&&S.wr.get(q.i)===idx)cls+=' wrong';
    }
    h+=`<button class="${cls}" onclick="check(${q.i},${idx},${isCor})" ${isAns?'disabled':''}>${ans}</button>`;
  });
  
  h+=`</div>`;
  
  if(isAns){
    h+=`<div class="mnemonic show">
      <div class="mnemonic-title">üí° –ö–∞–∫ –¥–∞ –∑–∞–ø–æ–º–Ω–∏—à:</div>
      <div>${q.m}</div>
      ${q.r&&q.r.length>0?'<div style="margin-top:12px"><strong>–°–≤—ä—Ä–∑–∞–Ω–∏:</strong><br>'+q.r.join('<br>')+'</div>':''}
    </div>`;
  }
  
  h+=`</div>`;
  
  document.getElementById('quizContainer').innerHTML=h;
  
  document.getElementById('prevBtn').disabled=S.ci===0;
  document.getElementById('nextBtn').disabled=S.ci>=fq.length-1;
  
  upd();
}

function lbl(ty){
  const l={'LOGIC':'üí° –õ–æ–≥–∏–∫–∞','NUMBERS':'üî¢ –¶–∏—Ñ—Ä–∏','RULES':'üìã –ü—Ä–∞–≤–∏–ª–∞','SIGNS':'üö¶ –ó–Ω–∞—Ü–∏','DOCUMENTS':'üìÑ –î–æ–∫—É–º–µ–Ω—Ç–∏'};
  return l[ty]||ty;
}

function check(id,idx,isCor){
  S.ans.add(id);
  if(isCor)S.cor.add(id);
  else S.wr.set(id,idx);
  save();
  render();
}

function next(){
  const fq=filt();
  if(S.ci<fq.length-1){
    S.ci++;
    render();
  }
}

function prev(){
  if(S.ci>0){
    S.ci--;
    render();
  }
}

function setupFilters(){
  document.querySelectorAll('.filter-btn').forEach(btn=>{
    btn.addEventListener('click',()=>{
      document.querySelectorAll('.filter-btn').forEach(b=>b.classList.remove('active'));
      btn.classList.add('active');
      S.f=btn.dataset.filter;
      S.ci=0;
      render();
    });
  });
}

function showTab(tab){
  document.querySelectorAll('.tab').forEach(t=>t.classList.remove('active'));
  document.querySelectorAll('.tab-content').forEach(t=>t.classList.remove('active'));
  event.target.classList.add('active');
  document.getElementById(tab+'-tab').classList.add('active');
  if(tab==='stats')updStats();
}

function updStats(){
  const byType={};
  Q.forEach(q=>{
    if(!byType[q.ty])byType[q.ty]={tot:0,cor:0,wr:0};
    byType[q.ty].tot++;
    if(S.cor.has(q.i))byType[q.ty].cor++;
    if(S.wr.has(q.i))byType[q.ty].wr++;
  });
  
  let h='<table class="cheat-table"><tr><th>–¢–∏–ø</th><th>–û–±—â–æ</th><th>–í–µ—Ä–Ω–∏</th><th>–ì—Ä–µ—à–Ω–∏</th><th>%</th></tr>';
  Object.keys(byType).forEach(ty=>{
    const d=byType[ty];
    const acc=d.cor+d.wr>0?Math.round((d.cor/(d.cor+d.wr))*100):0;
    h+=`<tr><td>${lbl(ty)}</td><td>${d.tot}</td><td>${d.cor}</td><td>${d.wr}</td><td>${acc}%</td></tr>`;
  });
  h+='</table>';
  document.getElementById('statsContent').innerHTML=h;
}

init();
</script>
</body>
</html>
